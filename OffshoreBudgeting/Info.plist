<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- MARK: App names
         CFBundleDisplayName is what users see. We'll set the full name here for macOS.
         iOS will override via its own InfoPlist.strings. -->
    <key>CFBundleDisplayName</key>
    <string>Offshore Budgeting</string>

    <!-- MARK: Internal bundle name
         Optional, internal/short name (not shown under the iOS icon). -->
    <key>CFBundleName</key>
    <string>Offshore</string>

    <!-- MARK: Background tasks -->
    <key>BGTaskSchedulerPermittedIdentifiers</key>
    <array>
        <string>com.mbrown.SoFar.refresh</string>
        <string>com.mbrown.SoFar.processing</string>
    </array>

    <!-- MARK: Background modes -->
    <key>UIBackgroundModes</key>
    <array>
        <string>remote-notification</string>
        <string>fetch</string>
        <string>processing</string>
    </array>
</dict>
</plist>
